@model List<AccountViewModel>
<!-- Create New -->
<div>
    @Html.ActionLink("Create", "Create", new { }, new { @class = "btn btn-default" })
</div>

<table class="tabletable-bordered table-hover dataTable clientSideExcelButtonDataTable" role="grid" id="account-table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
            <th>Address2</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Phone</th>
            <th>Fax</th>
            <th>Website</th>
            <th>ParentAccountId</th>
            <th>Lat</th>
            <th>Lng</th>
            <th>Description</th>
            <th>Active</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var record in Model)
        {
            <tr>
            <td>@record.Name</td>
            <td>@record.Address</td>
            <td>@record.Address2</td>
            <td>@record.City</td>
            <td>@record.State</td>
            <td>@record.Zip</td>
            <td>@record.Phone</td>
            <td>@record.Fax</td>
            <td>@record.Website</td>
            <td>@record.ParentAccountId</td>
            <td>@record.Lat</td>
            <td>@record.Lng</td>
            <td>@record.Description</td>
            <td>@record.Active</td>
            </tr>
        }
	</tbody>
</table>

